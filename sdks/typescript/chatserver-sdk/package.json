{
  "name": "@chatserver/sdk",
  "version": "0.10.0",
  "description": "TypeScript/JavaScript SDK for ChatServer - OpenAI-compatible API with multi-agent backend support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "api",
    "client",
    "sdk",
    "chat",
    "openai",
    "completion",
    "claude",
    "droid",
    "ccrouter",
    "typescript",
    "javascript"
  ],
  "author": "ChatServer Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coder/agentapi.git",
    "directory": "sdks/typescript/chatserver-sdk"
  },
  "homepage": "https://github.com/coder/agentapi#readme",
  "bugs": {
    "url": "https://github.com/coder/agentapi/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
